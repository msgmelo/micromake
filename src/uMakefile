CC = gcc

umake: umake.o arg_parse.o target.o list.o str_node.o
	${CC} -o umake umake.o arg_parse.o target.o list.o str_node.o
	echo done umake

umake.o: umake.c arg_parse.o target.o list.o str_node.o
	${CC} -c -o umake.o umake.c 
	echo done umake.o

arg_parse.o: arg_parse.c list.o
	${CC} -c -o arg_parse.o arg_parse.c 
	echo done arg_parse.o

target.o: target.c list.o str_node.o
	${CC} -c -o target.o target.c 
	echo done target.o

str_node.o: str_node.c list.o
	${CC} -c -o str_node.o str_node.c 
	echo done str_node.o

list.o: list.c
	${CC} -c -o list.o list.c
	echo done list.o
